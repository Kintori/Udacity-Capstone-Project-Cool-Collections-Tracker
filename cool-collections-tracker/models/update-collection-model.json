{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9+$]",
      "minLength": 1,
      "maxLength": 100
    },
    "description": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9+$]",
      "minLength": 1,
      "maxLength": 500
    }
  },
  "required": [
    "title",
    "description"
  ],
  "additionalProperties": false
}
